{
  "expo": {
    "name": "Perfect Roaster",
    "slug": "perfect-roaster",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "roaster",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grana.perfectroaster"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cristo_grana.perfectroaster"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Intro.otf",
            "assets/fonts/Poppins-Regular.ttf",
            "assets/fonts/Poppins-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": ["../../node_modules/@notifee/react-native/android/libs"]
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ed2d6772-1122-4d92-a90c-a06249a4db0a"
      }
    }
  }
}
